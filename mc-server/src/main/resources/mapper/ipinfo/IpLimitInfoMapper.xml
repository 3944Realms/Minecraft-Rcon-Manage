    </delete>

    <delete id="deleteIpLimitInfoByIds" parameterType="String">
        delete from ip_limit_info where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>